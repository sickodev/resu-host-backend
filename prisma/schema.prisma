generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Upload {
  id        String   @id @default(uuid(7))
  filename  String
  path      String
  shortlink String?
  createdAt DateTime @default(now())
  expiresAt DateTime
}
